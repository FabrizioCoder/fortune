{
	"name": "fortune",
	"version": "0.0.1",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "nodemon --watch src --watch test --exec \"pnpm run build && node ./test/index.js\" --ext ts,js"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"require": "./dist/index.js"
		}
	},
	"dependencies": {
		"@biscuitland/core": "^1.2.0"
	},
	"devDependencies": {
		"@types/node": "^18.6.4",
		"@typescript-eslint/eslint-plugin": "^5.32.0",
		"@typescript-eslint/parser": "^5.32.0",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"tsup": "^6.2.1",
		"typescript": "^4.7.4"
	},
	"license": "Apache-2.0",
	"author": "Vyrek",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/VyrekXD/fortune.git"
	},
	"bugs": {
		"url": "https://github.com/VyrekXD/fortune/issues"
	},
	"keywords": [
		"api",
		"discord",
		"bots",
		"typescript",
		"botdev",
		"biscuit",
		"biscuitland",
		"biscuitland/fortune"
	],
	"publishConfig": {
		"access": "public"
	}
}
